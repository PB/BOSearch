.list-group-item
  %h4.list-group-item-heading
    = place.name
  .list-group-item-text
    .country
      Country:
      = place.country_name
      .city
        City:
        = place.city_name
      .duration
        Duration:
        %span.begins
          = place.begins_at
        \-
        %span.ends
          = place.ends_at